extends layout

block content
  div(class='container')
    div(class='text-center')
        div(class='page-header')
            h1 #{title}
        img(src=image, class='img-responsive center-block')
        div(id='buttons-container', data-bind='foreach: options')
            input(class='btn btn-default', type='button', data-bind='value: latin, click: $parent.selected')